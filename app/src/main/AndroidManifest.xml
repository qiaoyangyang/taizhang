<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".OmsApplication"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tarket"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".SuccessfuActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <!-- 适配全面屏 -->
        <meta-data
            android:name="android.max_aspect"
            android:value="2.4" /> <!-- 适配华为（huawei）刘海屏 -->
        <meta-data
            android:name="android.notch_support"
            android:value="true" /> <!-- 适配小米（xiaomi）刘海屏 -->
        <meta-data
            android:name="notch.config"
            android:value="portrait|landscape" />

        <activity
            android:name=".activity.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".activity.LoginActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ForgetPwdActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ForgetPwdResetActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ForgetPwdFinishActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ForgetPwdGetCodeActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MainActivity"
            android:launchMode="singleInstance" />
        <activity android:name=".activity.OrderDisAddTipActivity" />
        <activity android:name=".activity.OrderChangeAddressActivity" />
        <activity android:name=".activity.OrderChangeAddressMapActivity" />
        <activity android:name=".activity.SearchActivity" />
        <activity
            android:name=".activity.OrderDisActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name=".activity.Search1Activity" />
        <activity android:name=".activity.PostDetailActivity" />
        <activity android:name=".activity.MyRechargeActivity" />
        <activity android:name=".activity.WriteOffActivity" />
        <activity android:name=".activity.MySettlementDetailActivity" />
        <activity android:name=".activity.ReleasePostActivity" />
        <activity android:name=".activity.RechargeFinishActivity" />
        <activity android:name=".activity.ModifyPassWordActivity" />
        <activity android:name=".activity.VoucherInspectionActivity" />
        <activity android:name=".activity.VoucherInspectionHistoryActivity" />
        <activity android:name=".activity.InputBoredomActivity" />

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.meiling.oms.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>
        <meta-data android:name="com.amap.api.v2.apikey" android:value="9a26d944277356b653bc2657adc35059">
        </meta-data>

        <service android:name="com.amap.api.location.APSService"/>

    </application>
    <queries>
        <package android:name="com.tencent.mm" />
        <package android:name="com.eg.android.AlipayGphone" />
        <package android:name="hk.alipay.wallet" />
    </queries>

</manifest>